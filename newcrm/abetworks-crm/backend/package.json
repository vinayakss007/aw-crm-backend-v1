{
  "name": "abetworks-crm-backend",
  "version": "1.0.0",
  "description": "Backend for ABETWORKS CRM - Enterprise-grade Customer Relationship Management System",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "migrate": "ts-node src/database/migrate.ts",
    "migrate:custom-fields": "ts-node src/database/migrate-custom-fields.ts",
    "migrate:custom-fields-management": "ts-node src/database/migrate-custom-fields-management.ts",
    "migrate:audit-logs": "ts-node src/database/migrate-audit-logs.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "keywords": [
    "crm",
    "enterprise",
    "typescript",
    "nodejs",
    "express",
    "postgresql"
  ],
  "author": "ABETWORKS CRM Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.0.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "joi": "^17.10.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "minio": "^7.1.3",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3",
    "typescript": "^5.2.2",
    "uuid": "^9.0.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.4",
    "@types/compression": "^1.7.3",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/express-validator": "^3.0.0",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash": "^4.14.199",
    "@types/minio": "^7.1.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.8.2",
    "@types/pg": "^8.10.3",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.5",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "^8.51.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
